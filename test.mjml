    <mj-style inline="inline">
        .link,
        a:not(.link--undercover) {
            color: #B91109;
        }
        .link--undercover {
            color: inherit;
        }
        .button {
            letter-spacing: 1px
        }
        h1,
        h2,
        h3,
        p {
            margin: 0;
        }
        h1 {
            font-size: 28px;
        }
        h2 {
